{
  "$schema": "node_modules/wrangler/config-schema.json",
  "compatibility_flags": ["nodejs_compat_v2"],
  "name": "appflare-backend",
  "main": "src/index.ts",
  "compatibility_date": "2025-02-04",
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "DURABLE_OBJECT",
        "class_name": "MongoDurableObject"
      }
    ]
  },
  "r2_buckets": [
    {
      "binding": "BUCKET",
      "bucket_name": "anglershield"
    }
  ],
  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["MongoDurableObject"]
    }
  ],
  "placement": {
    "mode": "smart"
  },
  "kv_namespaces": [
    {
      "binding": "KV",
      "id": "51ab2d7674b5490d9a34de6c14577f7e"
    }
  ]
}
